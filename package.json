{
  "name": "nodejs-clis",
  "version": "1.0.0",
  "description": "A collection of CLI utilities",
  "scripts": {
    "test": "mocha --compilers js:babel-register test/spec",
    "dev": "run-p watch:test watch:lint",
    "lint": "eslint ./",
    "watch:test": "nodemon --exec \"npm test || true\"",
    "watch:lint": "nodemon --exec \"npm run lint || true\"",
    "cli:echo": "babel-node echo.js",
    "cli:cat": "babel-node cat.js",
    "cli:touch": "babel-node touch.js",
    "cli:ls": "babel-node ls.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/lenguyenthanh/nodejs-clis.git"
  },
  "author": "Thanh Le",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CrabDude/nodejs-clis/issues"
  },
  "homepage": "https://github.com/CrabDude/nodejs-clis#readme",
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-nodev6": "^1.0.0",
    "babel-register": "^6.14.0",
    "lodash": "^4.16.4",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "songbird": "^1.0.4",
    "trycatch": "^1.5.21",
    "yargs": "^6.2.0"
  },
  "devDependencies": {
    "rimraf": "^2.5.4",
    "babel-eslint": "^7.0.0",
    "chai": "^3.5.0",
    "eslint": "^3.7.0",
    "eslint-config-noderiety": "^1.1.1",
    "mocha": "^3.0.2"
  }
}
